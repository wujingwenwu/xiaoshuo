@import "../../lib/wxParse/wxParse.wxss";

.page {
  display: block;
  min-height: 100%;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.wxParse{
  line-height: 80rpx;
}
.size{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background: rgb(226, 225, 225);
  display: flex;
  justify-content: space-around;
  align-items: center;
}
